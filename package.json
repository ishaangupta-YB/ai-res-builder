{
	"name": "ai-resume-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"auth:generate": "npx @better-auth/cli generate --config ./src/auth/index.ts --output ./src/db/auth.schema.ts",
		"auth:format": "npx prettier --write ./src/db/auth.schema.ts",
		"auth:update": "npm run auth:generate && npm run auth:format",
		"db:generate": "npx drizzle-kit generate",
		"db:migrate:dev": "wrangler d1 migrations apply ai-resume-app-db --local",
		"db:migrate:prod": "wrangler d1 migrations apply ai-resume-app-db --remote",
		"db:studio:dev": "npx drizzle-kit studio",
		"db:studio:prod": "npx drizzle-kit studio",
		"build:cf": "opennextjs-cloudflare build",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"clean": "rm -rf .next .open-next node_modules",
		"clean-deploy": "npm run clean && npm install && npm run deploy",
		"format": "npx prettier --write .",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
	},
	"dependencies": {
		"@ai-sdk/google": "^3.0.29",
		"@base-ui/react": "^1.2.0",
		"@cloudflare/workers-types": "^4.20260213.0",
		"@hookform/resolvers": "^5.2.2",
		"@opennextjs/cloudflare": "^1.15.1",
		"ai": "^6.0.86",
		"ai-gateway-provider": "^3.1.1",
		"aws4fetch": "^1.0.20",
		"better-auth": "^1.4.18",
		"better-auth-cloudflare": "^0.2.9",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"drizzle-orm": "^0.45.1",
		"embla-carousel-react": "^8.6.0",
		"framer-motion": "^12.34.0",
		"input-otp": "^1.4.2",
		"lucide-react": "^0.564.0",
		"next": "16.1.5",
		"next-themes": "^0.4.6",
		"radix-ui": "^1.4.3",
		"react": "19.1.5",
		"react-day-picker": "^9.13.2",
		"react-dom": "19.1.5",
		"react-hook-form": "^7.71.1",
		"react-image-crop": "^11.0.10",
		"react-resizable-panels": "^4.6.2",
		"react-to-print": "^3.2.0",
		"recharts": "^2.15.4",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"vaul": "^1.1.2",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.33",
		"@types/react": "^19.1.5",
		"@types/react-dom": "^19.1.5",
		"drizzle-kit": "^0.31.9",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"prettier": "^3.8.1",
		"shadcn": "^3.8.4",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.7.4",
		"wrangler": "^4.65.0"
	}
}
